{
  "name": "@better-auth-extended/preferences",
  "description": "Define and manage preferences with scoped settings.",
  "version": "0.1.1",
  "type": "module",
  "homepage": "https://better-auth-extended.jsolano.de/docs/plugins/preferences",
  "repository": {
    "type": "git",
    "url": "https://github.com/jslno/better-auth-extended.git",
    "directory": "packages/plugins/preferences"
  },
  "license": "MIT",
  "keywords": [],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --project tsconfig.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "better-auth-extended-dev-source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "better-auth-extended-dev-source": "./src/client.ts",
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "client": [
        "./dist/client.d.ts"
      ]
    }
  },
  "devDependencies": {
    "typescript": "catalog:",
    "tsdown": "catalog:",
    "@better-auth-extended/test-utils": "workspace:*",
    "vitest": "catalog:",
    "better-auth": "catalog:better-auth",
    "better-call": "catalog:",
    "@better-auth/utils": "catalog:better-auth",
    "better-sqlite3": "catalog:",
    "@types/better-sqlite3": "catalog:"
  },
  "dependencies": {
    "@better-auth-extended/internal-utils": "workspace:*",
    "zod": "catalog:"
  },
  "peerDependencies": {
    "better-auth": "catalog:better-auth"
  },
  "files": [
    "dist"
  ]
}
