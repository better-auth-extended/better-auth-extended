{
  "name": "@better-auth-extended/test-utils",
  "description": "Test utilities to assist you in writing code to test your Better-Auth plugins.",
  "version": "0.1.2",
  "type": "module",
  "homepage": "https://better-auth-extended.jsolano.de/docs/libraries/test-utils",
  "repository": {
    "type": "git",
    "url": "https://github.com/better-auth-extended/better-auth-extended.git",
    "directory": "packages/libraries/test-utils"
  },
  "license": "MIT",
  "keywords": [],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --project tsconfig.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "better-auth-extended-dev-source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@better-auth-extended/internal-utils": "workspace:*"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "tsdown": "catalog:",
    "vitest": "catalog:",
    "better-auth": "catalog:better-auth"
  },
  "peerDependencies": {
    "better-auth": "catalog:better-auth"
  },
  "files": [
    "dist"
  ]
}
