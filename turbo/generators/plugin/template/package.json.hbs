{
  "name": "@better-auth-extended/{{name}}",
  "description": "{{description}}",
  "version": "0.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/better-auth-extended/better-auth-extended.git",
    "directory": "packages/plugins/{{name}}"
  },
  "license": "MIT",
  "keywords": [],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --project tsconfig.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "better-auth-extended-dev-source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "better-auth-extended-dev-source": "./src/client.ts",
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "client": [
        "./dist/client.d.ts"
      ]
    }
  },
  "devDependencies": {
    "typescript": "catalog:",
    "tsdown": "catalog:",
    "@better-auth-extended/test-utils": "workspace:*",
    "vitest": "catalog:",
    "better-auth": "catalog:better-auth",
    "better-call": "catalog:"
  },
  "peerDependencies": {
    {{#if addSchema}}
    "zod": "catalog:",
    {{/if}}
    "better-auth": "catalog:better-auth"
  },
  "files": [
    "dist"
  ]
}
